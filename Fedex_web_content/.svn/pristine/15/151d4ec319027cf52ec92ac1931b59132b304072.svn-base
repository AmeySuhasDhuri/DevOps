.button{
z-index:2;
position:absolute;
margin-top:25px;

height:35px;
width:70px;
 text-align: center;
    text-decoration: none;
	color:white;
	background-color:#FF5D06;
	border-radius: 12px;
}
.button span {
  cursor: pointer;
  display: inline-block;
  position: relative;
  transition: 0.5s;
}

.button span:after {
  content: '\00bb';
  position: absolute;
  opacity: 0;
  top: 0;
  right: -20px;
  transition: 0.5s;
}

.button:hover span {
  padding-right: 25px;
}

.button:hover span:after {
  opacity: 1;
  right: 0;
}



